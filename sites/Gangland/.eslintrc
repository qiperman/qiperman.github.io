{
  "parser": "babel-eslint",
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "double"],
    "no-console": "off",
    "class-methods-use-this": "off",
    "no-unused-vars": "warn",
    "import/no-unresolved": "off",
    "import/no-extraneous-dependencies": "off",
    "comma-dangle": ["error", "never"],
    "object-property-newline": "error",
    "function-paren-newline": ["error", "never"]
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "plugins": [
    "html",
    "prettier"
  ],
  "extends": [
    "airbnb",
    "eslint:recommended"
  ]
}
